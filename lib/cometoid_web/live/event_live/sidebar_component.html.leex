<div class="title">
  <%= if @event.issue do @event.issue.title else @event.title end %>
</div>

<div class="inner">
  <br>
  <%= if @event.issue do %>
    <div><b>Issue Id:</b> <%= @event.issue.id %></div>
    <div><b>Issue Type:</b> <%= @event.issue.issue_type %></div>
    <br>
  <% end %>
  <div><b>Id:</b> <%= @event.id %></div>
  <div><b>Created at:</b> <%= @event.inserted_at %></div>
  <div><b>Changed at:</b> <%= @event.updated_at %></div>
</div>
